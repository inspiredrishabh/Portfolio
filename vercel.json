{
  "images": {
    "sizes": [640, 750, 828, 1080, 1200, 1920],
    "domains": [],
    "minimumCacheTTL": 60
  },
  "routes": [
    {
      "src": "/images/(.*)",
      "dest": "/public/images/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/public/assets/$1"
    },
    {
      "src": "/profile/(.*)",
      "dest": "/public/profile/$1"
    }
  ]
}
